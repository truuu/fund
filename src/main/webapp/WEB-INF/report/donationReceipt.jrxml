	<queryString>
		<![CDATA[SELECT  DISTINCT r.*, s.name, c.name AS corName, c.corporateNo,
  CONVERT(VARCHAR, DECRYPTBYPASSPHRASE(CONVERT(VARCHAR, $P{key1}), juminNo_encrypted)) juminNo, representative,
  s.homePostCode+' '+s.homeRoadAddress+' '+s.homeDetailAddress AS address, 
  c.postCode+' '+c.roadAddress+' '+c.detailAddress AS corAddress
FROM [Receipt] r LEFT JOIN [Sponsor] s ON r.sponsorID = s.id 
  LEFT JOIN [Payment] p ON r.id = p.receiptID
  LEFT JOIN [donationPurpose] d ON p.donationPurposeId = d.id
  LEFT JOIN [corporate] c ON d.corporateId = c.id
$P!{whereClause}]]>
	</queryString>
