	<parameter name="paymentList.jasper" class="java.lang.String"/>
	<parameter name="whereClause" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT  DISTINCT r.*, s.name, s.homeaddress AS address, c.name AS corName,c.corporateNo, t.no AS juminNo,
	 c.address AS corAddress,representative
FROM [Receipt] r LEFT JOIN [Sponsor] s ON r.sponsorID=s.id 
					LEFT JOIN [Payment] p ON r.id=p.receiptID
					LEFT JOIN [donationPurpose] d ON p.donationPurposeID=d.id
					LEFT JOIN [corporate] c ON d.corporateID = c.id
					JOIN tempNo t ON s.id=t.id
$P!{whereClause}]]>
	</queryString>
